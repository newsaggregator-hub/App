{
  "version": 2,
  "builds": [
    {
      "src": "api/signup.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/signup-free.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "features.html",
      "use": "@vercel/static"
    },
    {
      "src": "comparison.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/features",
      "dest": "/features.html"
    },
    {
      "src": "/comparison",
      "dest": "/comparison.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
